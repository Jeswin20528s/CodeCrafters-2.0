n = int(input("enter a number :"))
for i in range (1, n+1):
    for j in range(2,i):
        if(i%j==0):
            break
    else:
        print(i)
=========================
n = int(input("enter the number of rows for the triangle :"))
for i in range(n):
    for j in range(0 , i+1):
        print("*" ,end=" ")
    print()
===============================